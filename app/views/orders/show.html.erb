<center>
<table> 
  <th>Quantity</th>
  <th>Total_price</th>
  <th colspan=3>Actions</th>
  <tr>
    <td><%= @order.quantity %></td>
    <td><%= @order.total_price %></td>
    <td><%= button_to "Edit", edit_order_path(@order, @product),method: :get %></td>
    <td><%= button_to "Delete", order_path(@order.id),method: :delete %></td>
    <td><%= button_to "Back", product_path,method: :get %></td>
  </tr>
</table>
</center>
